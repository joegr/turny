# Production Environment Configuration
# Copy this to .env.production and fill in actual values

# Flask Configuration
FLASK_ENV=production
SECRET_KEY=generate-a-strong-random-key-here

# Database - Use managed PostgreSQL in production
DATABASE_URL=postgresql://user:password@production-db-host:5432/tournament_db

# HTTPS/Domain - Your actual domain
DOMAIN=yourdomain.com

# Google Cloud (if using Pub/Sub)
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=us-central1
